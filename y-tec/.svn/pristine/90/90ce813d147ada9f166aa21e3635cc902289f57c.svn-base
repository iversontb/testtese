.indexPage  .site__content{
    width: 100%;
}
.indexPage .content-wrap{
    padding-left: 0;
}
.slideWrap{
    background: url("../img/gall-container-back.png") repeat-x;
    margin-bottom: 12px;
}
.sliderContainer{
    width: 1000px;
    margin: 0 auto;
    height: 362px;
    position: relative;
}
.slidesjs-play,
.slidesjs-stop{
    display: none !important;
}
.slidesjs-previous{
    position: absolute;
    top: 143px;
    left: 0px;
    background: url("../img/gall-left-hover.png");
    width: 50px;
    height: 51px;
    z-index: 10;
    opacity: 0.5;
}
.slidesjs-next{
    position: absolute;
    top: 143px;
    right: 0px;
    background: url("../img/gall-right-hover.png");
    width: 50px;
    opacity: 0.5;
    z-index: 10;
    height: 51px;
}
.slidesjs-previous:hover,
.slidesjs-next:hover{
    opacity: 1;
}
.slidesjs-pagination{
    position: absolute;
    bottom: 43px;
    left: 503px;
    z-index: 10;
    margin-right: 6px;
}
.slidesjs-pagination li{
    float: left;
    margin-right: 6px;
}
.slidesjs-pagination li a.active{
    background: url("../img/dot-active.png");
}
.slidesjs-pagination li a{
    display: block;
    text-indent: -5000px;
    background: url("../img/dot.png");
    width: 14px;
    height: 14px;
}
.services{
    width: 1000px;
    font-family: 'Open Sans', sans-serif;
    margin: 0 auto;
    margin-bottom: 54px;
    background: url("../img/service-line.png") no-repeat 0 57px;
}
.services > ul > li{
    display: inline-block;
    width: 230px;
    padding-top: 146px;
    margin-right: 75px;
    vertical-align: top;
}
.services > ul > li:hover h3{
    color: #dc1f26;
}
.services > ul > li h3{
    font-size: 18px;
    margin-bottom: 24px;
    text-transform: uppercase;
}
.siteCreation{
    background: url("../img/site-creation.png") no-repeat top center;
}
.siteComersial{
    background: url("../img/site-comersial.png") no-repeat top center;
}
.siteMove{
    background: url("../img/site-move.png") no-repeat center 19px;
}
.services ol{
    margin-bottom: 29px;
}
.services ol li{
    margin-bottom: 10px;
}
.services ol a{
    color: #262223;
    font-size: 14px;
}
.services ol a:hover{
    color: #fff;
    background: #242021;
    padding: 0 10px;
    width: 100%;
    display: inline-block;
}
.knowMore:visited{
    color: #fff;
}
.knowMore{
    display: block;
    font-style: italic;
    white-space: pre-line;
    color: #fff;
    font-size: 14px;
    padding: 18px;
    background: #b8c1c8;
    border-radius: 5px;
}
.knowMore:hover{
    background: #dc1f26;
}
.services > ul{
    padding-left: 73px;
}
.reviewBlock{
    background: #f1f4f7;
    padding: 43px 0 0 0;
    margin-bottom: 51px;
}
.blockInside{
    width: 1000px;
    margin: 0 auto;
}
.review{
    width: 560px;
    display: inline-block;
    margin-right: 107px;
}
.review h3{
    color: #262223;
    text-transform: uppercase;
    font-family: 'Open Sans', sans-serif;
    font-size: 19px;
    font-weight: bold;
    margin-bottom: 31px;
}
.review h3 span{
    color: #dc1f26;
}
.reviewComment h4{
    color: #413e3f;
    margin-bottom: 11px;
    font-size: 16px;
    font-family: 'Open Sans', sans-serif;
}
.reviewComment p{
    color: #413e3f;
    font-size: 14px;
    line-height: 24px;
    font-style: italic;
}
.reviewComment p a:hover{
    text-decoration: none;
}
.reviewComment p a{
    font-style: normal;
    text-decoration: underline;
    color: #dc1f26;
}
.reviewComment{
    margin-bottom: 30px;
    border-bottom: 1px solid #e3e9eb;
    padding-bottom: 19px;
}
.blockInside .order{
    width: 329px;
    display: inline-block;
    border: none;
    vertical-align: top;
}
.orderWrap{
    background: #fff;
    padding: 20px 0 1px 0;
    border-bottom: 1px solid #e3e9eb;
}

.orderWrap > span {
    font-style: italic;
    font-size: 12px;
    font-family: "Trebuchet MS";
    color: #999999;
    display: block;
    padding-left: 19px;
    line-height: 16px;
    margin-bottom: 23px;
}
.orderWrap > span > span {
    color: #df3b42;
}
.orderWrap > p > span {
    display: block;
    color: #de343a;
    text-align: center;
    margin-bottom: 7px;
    font-family: 'Open Sans', sans-serif;
    font-size: 16px;
    font-weight: bold;
}
.orderWrap > p {
    font-size: 14px;
    font-family: 'Open Sans', sans-serif;
    text-align: center;
    line-height: 17px;
    margin-bottom: 15px;
}
.orderWrap > input {
    background: #de343a;
    display: block;
    border: none;
    border-radius: 4px;
    color: #fff;
    text-transform: uppercase;
    font-family: 'Ubuntu', sans-serif;
    font-size: 19px;
    width: 196px;
    height: 50px;
    margin-left: 72px;
    margin-bottom: 16px;
}
.orderWrap > input:hover {
    background: #e2494e;
}
.order fieldset > textarea,
.order fieldset > input{
    width: 255px;
}
.order > h2{
    padding-top: 0;
    padding-bottom: 36px;
}
.partners{
    width: 1000px;
    margin: 0 auto;
    margin-bottom: 55px;
}

.topicType{
    text-align: center;
    background: url("../img/topic-back.png") no-repeat left center;
    color: #262223;
    margin-bottom: 43px;
    font-size: 19px;
    font-family: 'Open Sans', sans-serif;
    text-transform: uppercase;
}
.topicType span{
    color: #df353b;
}


