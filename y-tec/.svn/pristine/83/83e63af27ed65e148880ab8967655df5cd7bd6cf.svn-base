        <header class="header">

            <!-- logo -->
            <a href="#" class="logo" title="на главную"><img src="img/logo.png" width="204" height="71" alt="Y-tec чтобы сайт приносил доход"></a>
            <!-- /logo -->

            <!-- menu -->
            <nav class="menu">
                <a href="#">О компании</a>
                <a href="#">Услуги</a>
